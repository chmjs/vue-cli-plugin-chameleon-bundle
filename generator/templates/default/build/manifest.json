{
  "name": "<%= bundleName %>",
  "namespace": "<%= bundleNamespace %>-",
  "version": 1,
  "styles": [
    "index.css"
  ],
  "scripts": [
    {
      "script": "bundle.js",
      "type": "components",
      "global": "__CHAMELEON_<%= bundleName.toUpperCase() %>__"
    },
    {
      "script": "bundle.meta.js",
      "type": "meta",
      "global": "__CHAMELEON_<%= bundleName.toUpperCase() %>_META__"
    }
  ],
  "helpPath": "help"
}